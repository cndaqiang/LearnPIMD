issn = {1520-5126},
journal = {J. Am. Chem. Soc.},
keywords = {Amines,Amines: chemistry,Catalysis,Cinchona Alkaloids,Cinchona Alkaloids: chemistry,Cyclization,Ketones,Ketones: chemistry,Models, Molecular,Molecular Conformation,Stereoisomerism},
month = {feb},
number = {5},
pages = {2116--27},
pmid = {25629689},
publisher = {American Chemical Society},
title = {{Origins of stereoselectivity in intramolecular aldol reactions catalyzed by cinchona amines.}},
url = {http://dx.doi.org/10.1021/ja513096x},
volume = {137},
year = {2015}
}
