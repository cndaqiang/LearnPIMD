pmid = {23746220},
publisher = {American Chemical Society},
title = {{A mechanistic rationale for the 9-amino(9-deoxy)epi cinchona alkaloids catalyzed asymmetric reactions via iminium ion activation of enones.}},
url = {http://dx.doi.org/10.1021/ja404784t},
volume = {135},
