title = {{Key role of electrostatic interactions in bacteriorhodopsin proton transfer}},
url = {http://dx.doi.org/10.1021/ja047982i http://pubs.acs.org/doi/abs/10.1021/ja047982i http://pubs.acs.org/doi/full/10.1021/ja047982i http://pubs.acs.org/doi/pdf/10.1021/ja047982i},
volume = {126},
