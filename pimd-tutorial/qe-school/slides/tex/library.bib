author = {Řeha, David and Kabel{\'{a}}č, Martin and Ryj{\'{a}}ček, Filip and {\v{S}}poner, Jiř{\'{\i}} and {\v{S}}poner, Judit E. and Elstner, Marcus and Suhai, S{\'{a}}ndor and Hobza, Pavel},
