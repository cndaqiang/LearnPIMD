keywords = {Bad water,DFTB,DFTB QMMM MD},
mendeley-tags = {Bad water,DFTB,DFTB QMMM MD},
month = {sep},
number = {38},
pages = {11007--27},
pmid = {25166899},
publisher = {American Chemical Society},
title = {{Molecular simulation of water and hydration effects in different environments: challenges and developments for DFTB based models.}},
