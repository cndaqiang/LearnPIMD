@article{Seifert2012,
abstract = {In this paper, we review the foundations of the density-functional tight-binding (DFTB) method. The method is based on the density-functional theory as formulated by Hohenberg and Kohn. It introduces several approximations: First, densities and potentials are written as superpositions of atomic densities and potentials. Second, many-center terms are summarized together with nuclear repulsion energy terms in a way that they can be written as a sum of pairwise repulsive terms. For small distances, the nuclear repulsion dominates, whereas for large distances, these terms vanish. The Kohn–Sham orbitals are expanded in a set of localized atom-centered functions. They are represented in a minimal basis of optimized atomic orbitals, which are obtained for spherical symmetric spin-unpolarized neutral atoms self-consistently. The whole Hamilton and overlap matrices contain one- and two-center contributions only. Therefore, they can be calculated and tabulated in advance as functions of the distance between atomic pairs. In addition, we discuss a self-consistent charge extension, the treatment of weak interactions, and a linear response scheme in connection with the DFTB method. Finally, some practical aspects are presented. © 2012 John Wiley {\&} Sons, Ltd.},
author = {Seifert, Gotthard and Joswig, Jan-Ole},
doi = {10.1002/wcms.1094},
file = {:home/riccardo/Documents/MendeleyPap//2012 - Seifert, Joswig - Density-functional tight binding—an approximate density-functional theory method.html:html},
issn = {1759-0884},
journal = {Wiley Interdiscip. Rev. Comput. Mol. Sci.},
language = {en},
number = {3},
pages = {456--465},
title = {{Density-functional tight binding—an approximate density-functional theory method}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/wcms.1094/abstract},
volume = {2},
year = {2012}
}
@article{Laino2006,
abstract = {A new linear-scaling method based on a multigrid approach to treat$\backslash$nlong-range electrostatic interactions in hybrid quantum mechanics/molecular$\backslash$nmechanics (QM/MM) simulations is described. The scheme has been implemented$\backslash$nin the context of a QM calculation based on density functional theory$\backslash$n(DFT). The method is tested on an analytical model to validate the$\backslash$nnew algorithm. Two realistic problems in $\alpha$-quartz crystals and a$\backslash$nzwitterionic dipeptide (GLY-ALA) in water have been chosen as further$\backslash$ntests. Results from QM/MM calculations with periodic boundary conditions$\backslash$n(PBC) show that the use of PBC is essential when studying highly$\backslash$nordered crystal structures, unless a carefully designed MM crystal$\backslash$nis used for the calculation. With a general shaped MM subsystem,$\backslash$nthe absence of PBC leads to an incorrect description of Kohn−Sham$\backslash$nband gaps and charge density. The present method allows periodic$\backslash$nboundary conditions to be used in molecular simulations of biological$\backslash$nand material science systems.},
author = {Laino, Teodoro and Mohamed, Fawzi and Laio, Alessandro and Parrinello, Michele},
