number = {1},
pages = {289--320},
pmid = {22191548},
title = {{Challenges for density functional theory}},
url = {http://dx.doi.org/10.1021/cr200107z http://pubs.acs.org/doi/abs/10.1021/cr200107z http://pubs.acs.org/doi/full/10.1021/cr200107z http://pubs.acs.org/doi/pdf/10.1021/cr200107z},
volume = {112},
