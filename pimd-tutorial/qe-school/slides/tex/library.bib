issn = {1932-6203},
journal = {PLoS One},
keywords = {DMD,Discrete Molecular Dynamics,Glycine,Glycine: chemistry,Humans,Intrinsically Disordered Proteins,Intrinsically Disordered Proteins: chemistry,Membrane Proteins,Membrane Proteins: chemistry,Molecular Dynamics Simulation,Nuclear Magnetic Resonance, Biomolecular,Protein Structure, Secondary,REM,Thermodynamics},
