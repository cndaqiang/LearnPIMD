keywords = {Chemical,Dispersion London VanDerWaals Review ToCite,Hydrogen Bonding,Models,Noble Gases,Noble Gases: chemistry,Quantum Theory,Thermodynamics,ToCite LC-BOP-LRD},
mendeley-tags = {Dispersion London VanDerWaals Review ToCite,ToCite LC-BOP-LRD},
month = {dec},
number = {22},
pages = {224104},
pmid = {20001021},
publisher = {AIP Publishing},
title = {{Density functional method including weak interactions: Dispersion coefficients based on the local response approximation.}},
url = {http://scitation.aip.org/content/aip/journal/jcp/131/22/10.1063/1.3269802},
