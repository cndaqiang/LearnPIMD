author = {Balamurugan, D and Aquino, Adelia J A and de Dios, Francis and Flores, Lionel and Lischka, Hans and Cheung, Margaret S},
doi = {10.1021/jp4026927},
issn = {1520-5207},
journal = {J. Phys. Chem. B},
keywords = {Biological,REM},
mendeley-tags = {Biological,REM},
month = {oct},
number = {40},
pages = {12065--75},
pmid = {24033226},
publisher = {American Chemical Society},
title = {{Multiscale simulation of the ground and photo-induced charge-separated states of a molecular triad in polar organic solvent: exploring the conformations, fluctuations, and free energy landscapes.}},
