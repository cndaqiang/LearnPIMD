pages = {14791--14795},
title = {{Collective many-body van der Waals interactions in molecular systems}},
url = {http://www.pnas.org/content/109/37/14791 http://www.pnas.org/content/109/37/14791.full http://www.pnas.org/content/109/37/14791.full.pdf http://www.pnas.org/content/109/37/14791.short},
