journal = {J. Am. Chem. Soc.},
keywords = {Alkynes,Alkynes: chemistry,Azides,Azides: chemistry,Catalysis,Cyclization,Organometallic Compounds,Organometallic Compounds: chemistry,Ruthenium,Ruthenium: chemistry,Triazoles,Triazoles: chemical synthesis},
month = {jul},
number = {28},
pages = {8923--30},
pmid = {18570425},
publisher = {American Chemical Society},
title = {{Ruthenium-catalyzed azide-alkyne cycloaddition: scope and mechanism.}},
url = {http://dx.doi.org/10.1021/ja0749993},
volume = {130},
year = {2008}
}
