@article{Dirac1930,
abstract = {ABSTRACT For dealing with atoms involving many electrons the accurate$\backslash$nquantum theory, involving a solution of the wave equation in many-dimensional$\backslash$nspace, is far too complicated to be practicable. One must therefore$\backslash$nresort to approximate methods. The best of these is Hartree's method$\backslash$nof the self-consistent field. Even this, however, is hardly practicable$\backslash$nwhen one has to deal with very many electrons, so that one then requires$\backslash$na still simpler and rougher method. Such a method is provided by$\backslash$nThomas' atomic model, in which the electrons are regarded as forming$\backslash$na perfect gas satisfying the Fermi statistics and occupying the region$\backslash$nof phase space of lowest energy. This region of phase space is assumed$\backslash$nto be saturated, with two electrons with opposite spins in each volume$\backslash$n(2$\pi$h)3, and the remainder is assumed to be empty. Although this model$\backslash$nhitherto has not been justified theoretically, it seems to be a plausible$\backslash$napproximation for the interior of a heavy atom and one may expect$\backslash$nit to give with some accuracy the distribution of electric charge$\backslash$nthere.},
author = {Dirac, P. a. M.},
doi = {10.1017/S0305004100016108},
isbn = {0305004100016},
issn = {0305-0041},
journal = {Math. Proc. Cambridge Philos. Soc.},
keywords = {LDA,LSDA,ST130,ToCite LSDA},
mendeley-tags = {LDA,LSDA,ST130,ToCite LSDA},
number = {03},
pages = {376},
pmid = {731},
title = {{Note on Exchange Phenomena in the Thomas Atom}},
volume = {26},
year = {1930}
}
