mendeley-tags = {Folder - DFT,Folder - dDsC,{\_}tablet},
month = {jul},
number = {7},
pages = {1990--2001},
title = {{A System-Dependent Density-Based Dispersion Correction}},
url = {http://dx.doi.org/10.1021/ct1001494 http://pubs.acs.org/doi/abs/10.1021/ct1001494 http://pubs.acs.org/doi/full/10.1021/ct1001494 http://pubs.acs.org/doi/pdf/10.1021/ct1001494 http://pubs.acs.org/doi/pdfplus/10.1021/ct1001494},
