@article{Heaton-Burgess2010,
abstract = {The ground state structure of C(4N+2) rings is believed to exhibit a geometric transition from angle alternation (N < or = 2) to bond alternation (N > 2). All previous density functional theory (DFT) studies on these molecules have failed to reproduce this behavior by predicting either that the transition occurs at too large a ring size, or that the transition leads to a higher symmetry cumulene. Employing the recently proposed perspective of delocalization error within DFT we rationalize this failure of common density functional approximations (DFAs) and present calculations with the rCAM-B3LYP exchange-correlation functional that show an angle-to-bond-alternation transition between C(10) and C(14). The behavior exemplified here manifests itself more generally as the well known tendency of DFAs to bias toward delocalized electron distributions as favored by Huckel aromaticity, of which the C(4N+2) rings provide a quintessential example. Additional examples are the relative energies of the C(20) bowl, cage, and ring isomers; we show that the results from functionals with minimal delocalization error are in good agreement with CCSD(T) results, in contrast to other commonly used DFAs. An unbiased DFT treatment of electron delocalization is a key for reliable prediction of relative stability and hence the structures of complex molecules where many structure stabilization mechanisms exist.},
author = {Heaton-Burgess, Tim and Yang, Weitao},
doi = {10.1063/1.3445266},
isbn = {doi:10.1063/1.3445266},
issn = {1089-7690},
journal = {J. Chem. Phys.},
keywords = {ST35},
mendeley-tags = {ST35},
month = {jun},
number = {23},
pages = {234113},
pmid = {20572695},
publisher = {AIP Publishing},
title = {{Structural manifestation of the delocalization error of density functional approximations: C(4N+2) rings and C(20) bowl, cage, and ring isomers.}},
url = {http://scitation.aip.org/content/aip/journal/jcp/132/23/10.1063/1.3445266},
volume = {132},
year = {2010}
}
@article{Zacharias2008,
abstract = {Coarse-grained elastic network models (ENM) of proteins can be used$\backslash$nefficiently to explore the global mobility of a protein around a$\backslash$nreference structure. A new Hamiltonian-replica exchange molecular$\backslash$ndynamics (H-RexMD) method has been designed that effectively combines$\backslash$ninformation extracted from an ENM analysis with atomic-resolution$\backslash$nMD simulations. The ENM analysis is used to construct a distance-dependent$\backslash$npenalty (flooding or biasing) potential that can drive the structure$\backslash$naway from its current conformation in directions compatible with$\backslash$nthe ENM model. Various levels of the penalty or biasing potential$\backslash$nare added to the force field description of the MD simulation along$\backslash$nthe replica coordinate. One replica runs at the original force field.$\backslash$nBy focusing the penalty potential on the relevant soft degrees of$\backslash$nfreedom the method avoids the rapid increase of the replica number$\backslash$nwith increasing system size to cover a desired temperature range$\backslash$nin conventional (temperature) RexMD simulations. The application$\backslash$nto domain motions in lysozyme of bacteriophage T4 and to peptide$\backslash$nfolding indicates significantly improved conformational sampling$\backslash$ncompared to conventional MD simulations.},
author = {Zacharias, Martin},
doi = {10.1021/ct7002258},
