journal = {J. Phys. Chem. A},
keywords = {Computer Simulation,Dimerization,Models, Chemical,Molecular Structure,Polycyclic Hydrocarbons, Aromatic,Polycyclic Hydrocarbons, Aromatic: chemistry,Quantum Theory,ST258,ToCite DCACP},
mendeley-tags = {ST258,ToCite DCACP},
month = {oct},
number = {43},
pages = {10968--76},
pmid = {18828578},
publisher = {American Chemical Society},
title = {{Interactions in large, polyaromatic hydrocarbon dimers: application of density functional theory with dispersion corrections.}},
