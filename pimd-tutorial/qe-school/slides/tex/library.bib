journal = {J. Chem. Phys.},
keywords = {Atomic electronic properties,Carbon,Chemical bonds,Electron correlation calculations,Statistical properties,wave functions},
mendeley-tags = {Atomic electronic properties,Carbon,Chemical bonds,Electron correlation calculations,Statistical properties,wave functions},
month = {dec},
