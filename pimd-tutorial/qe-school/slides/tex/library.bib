journal = {Proc. Natl. Acad. Sci. U. S. A.},
keywords = {Algorithms,Dipeptides,Dipeptides: chemistry,Models, Chemical,Monte Carlo Method,Protein Conformation,Proteins,Proteins: chemistry,Solutions,Solutions: chemistry,Water,Water: chemistry,toCite HREM},
mendeley-tags = {toCite HREM},
