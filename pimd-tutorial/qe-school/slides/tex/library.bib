title = {{A density-functional study of the intermolecular interactions of benzene}},
url = {http://scitation.aip.org/content/aip/journal/jcp/105/19/10.1063/1.472649 http://jcp.aip.org/resource/1/jcpsa6/v105/i19/p8684{\_}s1},
volume = {105},
